    <script src="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.js"></script>
    <script>
        AOS.init({
            duration: 800,
            easing: 'ease',
            once: true,
            offset: 100
        });

        // Modal AIO
        function openAIOModal(event) {
            event.preventDefault();
            const modal = document.getElementById('aioModal');
            modal.style.display = 'block';
            setTimeout(() => {
                modal.classList.add('show');
            }, 10);
            document.body.style.overflow = 'hidden';
        }

        function closeAIOModal() {
            const modal = document.getElementById('aioModal');
            modal.classList.remove('show');
            setTimeout(() => {
                modal.style.display = 'none';
                document.body.style.overflow = 'auto';
            }, 300);
        }

        // Fermer le modal en cliquant en dehors
        document.getElementById('aioModal').addEventListener('click', function(event) {
            if (event.target === this) {
                closeAIOModal();
            }
        });

        // Fermer le modal avec la touche Echap
        document.addEventListener('keydown', function(event) {
            if (event.key === 'Escape') {
                closeAIOModal();
            }
        });

        // Fermer avec le bouton X
        document.querySelector('.close-modal').addEventListener('click', closeAIOModal);
    </script>

