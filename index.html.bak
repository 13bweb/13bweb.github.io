    <script>
    function openAIOModal(event) {
        event.preventDefault();
        document.getElementById('aioModal').style.display = 'block';
        document.body.style.overflow = 'hidden';
    }

    function closeAIOModal() {
        document.getElementById('aioModal').style.display = 'none';
        document.body.style.overflow = 'auto';
    }

    window.onclick = function(event) {
        var modal = document.getElementById('aioModal');
        if (event.target == modal) {
            closeAIOModal();
        }
    }
    </script>

